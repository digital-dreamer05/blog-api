        border-radius: 15px;
        padding: 30px;
        position: relative;
        overflow: hidden;
        transition: all 0.3s ease;
        cursor: pointer;
      }

      .card:hover {
        transform: translateY(-10px);
        box-shadow: 0 20px 40px rgba(0, 255, 65, 0.3);
        border-color: #39ff14;
      }

      .card::before {
        content: '';
        position: absolute;
        top: -2px;
        left: -2px;
        right: -2px;
        bottom: -2px;
        background: linear-gradient(45deg, #00ff41, #39ff14, #32cd32, #00ff41);
        border-radius: 15px;
        opacity: 0;
        z-index: -1;
        animation: borderGlow 3s linear infinite;
      }

      .card:hover::before {
        opacity: 0.7;
      }

      @keyframes borderGlow {
